<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
    <com.wiz.dev.wiztalk.apppush.view.RcdProgressView_
        android:id="@+id/viewRcdProgress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:layout_gravity="center"/>
	
    <com.wiz.dev.wiztalk.apppush.view.RcdVoiceView_
        android:id="@+id/viewRcdVoice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:layout_gravity="center"/>
    
    <com.wiz.dev.wiztalk.apppush.view.RcdCancelView_
        android:id="@+id/viewRcdCancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:layout_gravity="center"/>
    
    <com.wiz.dev.wiztalk.apppush.view.RcdWarningView_
        android:id="@+id/viewRcdWarning"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:layout_gravity="center"/>

</FrameLayout>
